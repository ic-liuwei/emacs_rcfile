setenv PATH "${PATH}:${HOME}/bin:${HOME}/.local/bin"

alias cls "clear"
alias h "history"
alias ll "ls -l"
alias la "ls -al"
alias rm "rm -i"
alias cp "cp -i"
alias mv "mv -i"

alias f "firefox &"
alias e "emacs \!* &"
alias ooffice "ooffice \!* &"
alias o "ooffice"
alias gs "git status"
alias ga "git add"

alias cd. "cd ../"
alias cd.. "cd ../../"


alias vam "emacs ~/.cshrc &"
alias sc "source ~/.cshrc"